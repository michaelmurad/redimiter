{
  "name": "redimiter",
  "version": "0.1.0",
  "description": "A Redis based Express middleware and Node rate limiter",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/michaelmurad/redimiter.git"
  },
  "bugs": {
    "url": "https://github.com/michaelmurad/redimiter/issues"
  },
  "homepage": "https://github.com/michaelmurad/redimiter",
  "scripts": {
    "compile": "tsc",
    "pretest": "npm run compile",
    "test": "nyc mocha --check-leaks ./dist/test/",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "testPromise":
      "npm run compile && mocha --check-leaks --grep rateLimiterPromise  ./dist/test/*.js --exit",
    "testRedis":
      "npm run compile && mocha --check-leaks --grep Redis  ./dist/test/*.js --exit"
  },
  "keywords": [
    "redis",
    "express",
    "rate",
    "limiter",
    "rate-limiter",
    "middleware"
  ],
  "author": "Michael Murad",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/express": "^4.11.1",
    "@types/node": "^9.4.2",
    "@types/sinon": "^4.1.3",
    "chai": "^4.1.2",
    "chalk": "^2.3.1",
    "coveralls": "^3.0.0",
    "express": "^4.16.2",
    "express3": "0.0.0",
    "ioredis": "^3.2.2",
    "mocha": "^5.0.0",
    "nyc": "^11.4.1",
    "prettier": "^1.10.2",
    "redis": "^2.8.0",
    "sinon": "^4.2.2",
    "supertest": "^3.0.0",
    "typescript": "^2.7.1"
  },
  "typings": "dist/index.d.ts",
  "dependencies": {}
}
